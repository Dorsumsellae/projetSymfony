{% extends 'base.html.twig' %}

{% block title %}Formulaire Upload Fichier
{% endblock %}

{% block main %}

	<h2>Alert :</h2>
	{% if  alert != "" %}
		<div class="alert alert-warning" role="alert">
			{{alert}}
		</div>
	{% endif %}

	<h2>Status</h2>
	{% if  status != "" %}
		<div class="alert alert-secondary" role="alert">
			{{status}}
		</div>
	{% endif %}
{% endblock %}
